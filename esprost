Sabadon:
  Type: VINDICATOR
  Display: '&dSabadon Gardien du néant'
  Health: 160
  Damage: 20
  Skills:
  - message{cooldown=20;m="<mob.name><&co> Bienvenue dans le néant !"} @PlayersInRadius{r=40} ~onCombat >0 0.2
  - message{m="<mob.name><&co> Tu est faible, <trigger.name>!"} @PlayersInRadius{r=40} ~onPlayerKill >0 1
  - skill{s=SmashAttack} @Target >0 0.3
  - skill{s=Primes} @Target 0.9
  - skill{s=Primes} @Target 0.4
  - skill{s=AngrySludgePoison} @Target 0.1
  - skill{s=Sabadon} @Target 0.8
  Equipment:
  - KingsCrown:4
  - Nikanaxe:0
  Drops:
  - SkeletonKingDrops
  - Nikanaxe
  - Nikanaxe
  - Nikanaxe
  - Nikanaxe
