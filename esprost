esprost1:
  Type: ELDER_GUARDIAN
  Display: '&dHydriades'
  Health: 100
  Damage: 6
  BossBar:
   Enabled: true
   Title: 'Hydriades'
   Range: 20
   Color: BLUE
   Style: SOLID
  Skills:
  - message{cooldown=20;m="<mob.name><&co> Nous allons voir si tu est aussi fort que dans l'eau  "} @PlayersInRadius{r=40} ~onCombat >0 0.2
  - skill{s=SmashAttack} @Target >0 0.1
  - skill{s=colonedeau} @Target >0 0.1
  Equipment:
  - Nikana
  Drops:
  - Nikana
